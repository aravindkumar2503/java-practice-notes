import java.util.Scanner;
class  pattern_exercise
{
	static Scanner sc=new Scanner(System.in);
	public static void main(String[] args) 
	{
		System.out.print("enter n:");
		int n=sc.nextInt();
		for (int i=0;i<n ;i++ )
		{
			//int val=((i%2==0)?1:0);
			int a=(i%2==0)?1:0;
			for (int j=0;j<n;j++ )
			{
				
				if(i<=j)
					System.out.print(a+" ");
				a=1-a;
				
			}
			System.out.println();
		}
		
	}
}
