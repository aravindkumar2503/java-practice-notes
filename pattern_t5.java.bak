import java.util.Scanner;
class  pattern_t5
{
	static  Scanner sc=new Scanner(System.in);
	public static void main(String[] args) 
	{

		System.out.print("Enter the num:");
		int n=sc.nextInt();
		for(int i=0;i<n;i++)
		{
			for (int j=0;j<n;j++ )
			{
				if(i==0&&j==3||i==1&&j==2||i==2&&j==1||i==3&&j==0||i==4&&j==1||i==5&&j==2||i==6&&j==3||i==1&&j==4||i==2&&j==5||i==3&&j==6||i==4&&j==5)
					System.out.print("* ");
				else
					System.out.print("  ");
			}
			System.out.println();
		}
		
	}
}
